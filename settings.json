{
    "rust-analyzer.check.command": "clippy",
    "editor.rulers": [
        100
    ],
    "markdownlint.config": {
        "MD030": {
            "ol_multi": 2,
            "ul_multi": 3
        },
        "MD007": {
            "indent": 4
        },
        "MD013": {
            "line_length": 100,
            "code_blocks": false,
            "headings": false,
            "tables": false
        }
    },
    "workbench.colorCustomizations": {
        "tab.activeBorder": "#a855f7",
        "tab.activeBorderTop": "#a855f7",
        "tab.activeBackground": "#2d1b4e",
        "tab.activeForeground": "#e9d5ff",
        "tab.border": "#4c1d95",
        "editorGroup.border": "#a855f7",
        "editorGroupHeader.tabsBorder": "#a855f7",
        "focusBorder": "#a855f7",
        "panel.border": "#a855f7",
        "sideBar.border": "#a855f7",
        "activityBar.border": "#a855f7",
        "statusBar.border": "#a855f7",
        "editorGroupHeader.border": "#a855f7",
        "sash.hoverBorder": "#a855f7",
        "titleBar.border": "#a855f7"
    },
    "window.titleBarStyle": "custom",
    "workbench.editor.showTabs": "multiple"
}
